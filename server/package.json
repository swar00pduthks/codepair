{
  "name": "codepair-server",
  "version": "1.0.0",
  "description": "Yjs WebSocket server for real-time collaborative pair programming",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/server.ts",
    "start": "node dist/server.js",
    "watch": "nodemon --exec ts-node src/server.ts"
  },
  "keywords": [
    "yjs",
    "websocket",
    "collaboration",
    "pair-programming",
    "realtime"
  ],
  "author": "CodePair Team",
  "license": "MIT",
  "dependencies": {
    "y-websocket": "^1.5.0",
    "ws": "^8.14.2",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.0",
    "nodemon": "^3.0.2"
  }
} 